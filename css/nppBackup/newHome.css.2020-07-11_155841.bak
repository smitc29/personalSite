.leftbox
{
	position: absolute;
	top: 5vh;
	left: 0px;
	height: 10vh;
	width: 10vh;
	background-color: rgba(167,12,14,0.00);
	
	/* Safari 4.0 - 8.0 */
    -webkit-animation-name: LeftBox;
    -webkit-animation-duration: 10.0s;
    -webkit-animation-timing-function: linear;
    -webkit-animation-delay: 0s;
    -webkit-animation-iteration-count: infinite;

    /* Standard syntax */
    animation-name: LeftBox;
    animation-duration: 10.0s;
    animation-timing-function: linear;
    animation-delay: 0s;
    animation-iteration-count: infinite;
	animation-direction: forwards;
	animation-fill-mode: forwards;
}

@-webkit-keyframes LeftBox {
    0%   {background-color: rgba(167,12,14,0.50); left: -20%;}
    100% {background-color: rgba(167,12,14,0.0); left: 20%;}
}

/* Standard syntax */
@keyframes LeftBox {
   0%   {background-color: rgba(167,12,14,0.50); left: -20%;}
    100% {background-color: rgba(167,12,14,0.0); left: 20%;}
}

.rightbox
{
	position: absolute;
	top: 5vh;
	right: 0px;
	height: 10vh;
	width: 10vh;
	background-color: rgba(167,12,14,0.00);
	border-radius: 5vh;
	
	/* Safari 4.0 - 8.0 */
    -webkit-animation-name: RightBox;
    -webkit-animation-duration: 10.0s;
    -webkit-animation-timing-function: linear;
    -webkit-animation-delay: 0s;
    -webkit-animation-iteration-count: infinite;

    /* Standard syntax */
    animation-name: RightBox;
    animation-duration: 10.0s;
    animation-timing-function: linear;
    animation-delay: 0s;
    animation-iteration-count: infinite;
	animation-direction: forwards;
	animation-fill-mode: forwards;
}

@-webkit-keyframes RightBox {
    0%   {background-color: rgba(167,12,14,0.50); right: -20%;}
    100% {background-color: rgba(167,12,14,0.0); right: 20%;}
}

/* Standard syntax */
@keyframes RightBox {
   0%   {background-color: rgba(167,12,14,0.50); right: -20%;}
    100% {background-color: rgba(167,12,14,0.0); right: 20%;}
}

@-webkit-keyframes FadeIn {
    0%   {opacity: 0; color:rgba(255,255,255,0.00);}
	90%   {opacity: 0; color:rgba(255,255,255,0.00);}
    100% {opacity: 1; color:rgba(255,255,255,1.00);}
}

/* Standard syntax */
@keyframes FadeIn {
   0%   {opacity: 0; color:rgba(255,255,255,0.00);}
	90%   {opacity: 0; color:rgba(255,255,255,0.00);}
    100% {opacity: 1; color:rgba(255,255,255,1.00);}
}


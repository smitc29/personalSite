@charset "UTF-8";
/* CSS Document */
html
{
	overflow-x:hidden;
}

body
{
	min-height: 500vh;
	max-width: 100vw;
	font-family: 'Roboto Slab', serif;
	background-color:rgba(248,243,243,1.00);
	
	overflow-y: hidden;
	overflow-x: hidden;
	z-index: -3;
	/*outline: 1px solid red;*/
}

aside
{
	display: none;
}

article /* Only include one of these, everything else goes inside */
{
	position: absolute;
	top: 100vh;
	width: 100vw;
	min-height: 20px;
	background-color: none;	
}

section
{
	position: relative;
	width: 100vw;	
	/*background-color: rgba(151,0,2,1.00) ;*/
	padding-top: 120px;
	padding-bottom: 40px;
	margin-top: 240px;
	min-height: 100vh;
	overflow-y: visible;
	overflow-x: hidden;
}

a
{
	text-decoration: none;
}

ul
{
list-style: none;
}

hr
{
	margin: 50px 10px 10px 10px;
}

h1
{
	
}

h2
{
	position: relative;
	margin: 0 auto;
	text-align: center;
	font-size: 25pt;
}

h3
